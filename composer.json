{
    "name": "protung/open-api-generator",
    "description": "Open Api specification generator.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "open api schema",
        "open api schema generator",
        "swagger schema generator",
        "swagger schema",
        "openapi",
        "open api",
        "schema",
        "swagger",
        "json",
        "generator"
    ],
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.de"
        },
        {
            "name": "Cezary Stepkowski",
            "email": "cezary.stepkowski@protonmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-json": "*",
        "azjezz/psl": "^2.1.0",
        "cebe/php-openapi": "^1.7.0",
        "jms/serializer": "^3.18.2",
        "myclabs/deep-copy": "^1.11.0",
        "phpstan/phpdoc-parser": "^1.13.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "webmozart/assert": "^1.11.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^10.0.0",
        "ergebnis/composer-normalize": "^2.28.3",
        "php-standard-library/psalm-plugin": "^2.0.2",
        "phpstan/phpstan": "^1.9.1",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "phpstan/phpstan-strict-rules": "^1.4.4",
        "phpstan/phpstan-symfony": "^1.2.16",
        "phpstan/phpstan-webmozart-assert": "^1.2.2",
        "phpunit/phpunit": "^9.5.26",
        "psalm/plugin-phpunit": "^0.18.3",
        "roave/security-advisories": "dev-master",
        "symfony/config": "^5.4 || ^6.1.3",
        "symfony/var-dumper": "^5.4 || ^6.1.6",
        "thecodingmachine/phpstan-strict-rules": "^1.0.0",
        "vimeo/psalm": "^4.30.0"
    },
    "conflict": {
        "nikic/php-parser": "<4.13.2",
        "squizlabs/php_codesniffer": "<3.7.1"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Protung\\OpenApiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Protung\\OpenApiGenerator\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
