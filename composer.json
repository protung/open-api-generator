{
    "name": "protung/open-api-generator",
    "description": "Open Api specification / schema generator.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "open api schema",
        "open api schema generator",
        "swagger schema generator",
        "swagger schema",
        "openapi",
        "open api",
        "schema",
        "swagger",
        "json",
        "generator"
    ],
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.de"
        },
        {
            "name": "Cezary Stepkowski",
            "email": "cezary.stepkowski@protonmail.com"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
        "ext-json": "*",
        "azjezz/psl": "^2.9.1 || ^3.0.0",
        "devizzent/cebe-php-openapi": "^1.1.0",
        "jms/metadata": "^2.8.0",
        "jms/serializer": "^3.30.0",
        "myclabs/deep-copy": "^1.10",
        "phpstan/phpdoc-parser": "^2.0.0",
        "symfony/form": "^6.4 || ^7.1",
        "symfony/routing": "^6.4 || ^7.1",
        "symfony/validator": "^6.4 || ^7.1",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/coding-standard": "^13.0.0",
        "ergebnis/composer-normalize": "^2.45.0",
        "php-standard-library/phpstan-extension": "^2.0",
        "phpstan/phpstan": "^2.1.10",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "phpstan/phpstan-strict-rules": "^2.0.4",
        "phpstan/phpstan-symfony": "^2.0.3",
        "phpstan/phpstan-webmozart-assert": "^2.0.0",
        "phpunit/phpunit": "^11.5.15",
        "roave/security-advisories": "dev-master",
        "symfony/config": "^6.4 || ^7.2",
        "symfony/options-resolver": "^6.4 || ^7.2",
        "symfony/var-dumper": "^6.4 || ^7.2"
    },
    "conflict": {},
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Protung\\OpenApiGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Protung\\OpenApiGenerator\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
